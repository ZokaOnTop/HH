local maxGlitchLines = 5;
local currentGlitchLines = 0;
local glitchDebounce = false;
local function fullScreenGlitch(customColor)
	if (currentGlitchLines >= maxGlitchLines) then
		return;
	end
	local parent = game:GetService("CoreGui"):FindFirstChild("ZokaLite") or screenGui;
	if not parent then
		return;
	end
	currentGlitchLines = currentGlitchLines + 1;
	local line = Instance.new("Frame");
	line.Name = "ZokaGlitchLine";
	line.Parent = parent;
	local baseColor = customColor or Color3.fromRGB(0, 255, 100);
	line.BackgroundColor3 = ((math.random() > 0.5) and baseColor) or Color3.fromRGB(255, 255, 255);
	line.BorderSizePixel = 0;
	line.BackgroundTransparency = 0.4;
	line.Size = UDim2.new(1, 0, 0, math.random(1, 4));
	line.Position = UDim2.new(0, 0, math.random(5, 95) / 100, 0);
	line.ZIndex = 2000000;
	task.delay(0.03, function()
		if (line and line.Parent) then
			line:Destroy();
		end
		currentGlitchLines = currentGlitchLines - 1;
	end);
end
local function activationEffect()
	if glitchDebounce then
		return;
	end
	glitchDebounce = true;
	playSound(SOUNDS.Static, 0.6, 1.2);
	for i = 1, 2 do
		fullScreenGlitch();
		task.wait(0.05);
	end
	task.delay(0.2, function()
		glitchDebounce = false;
	end);
end
