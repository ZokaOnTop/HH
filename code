local function createCategory(name)
	local catLabel = Instance.new("TextLabel", container);
	catLabel.Name = "Category_" .. name;
	catLabel.Size = UDim2.new(1, 0, 0, 25);
	catLabel.BackgroundTransparency = 1;
	catLabel.Text = "--- [ " .. name:upper() .. " ] ---";
	catLabel.TextColor3 = Color3.fromRGB(0, 255, 100);
	catLabel.Font = Enum.Font.Code;
	catLabel.TextSize = 14;
	catLabel.ZIndex = 7;
	local buttonGridFrame = Instance.new("Frame", container);
	buttonGridFrame.Name = name .. "_Grid";
	buttonGridFrame.BackgroundTransparency = 1;
	buttonGridFrame.Size = UDim2.new(1, 0, 0, 0);
	local grid = Instance.new("UIGridLayout", buttonGridFrame);
	grid.CellSize = UDim2.new(0, 160, 0, 35);
	grid.CellPadding = UDim2.new(0, 10, 0, 10);
	grid.SortOrder = Enum.SortOrder.LayoutOrder;
	grid:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
		buttonGridFrame.Size = UDim2.new(1, 0, 0, grid.AbsoluteContentSize.Y);
	end);
	return buttonGridFrame;
end
local function createButton(name, parentGrid, callback)
	local btn = Instance.new("TextButton", parentGrid);
	btn.Name = name .. "Btn";
	btn.BackgroundColor3 = Color3.fromRGB(15, 15, 15);
	btn.TextColor3 = Color3.fromRGB(0, 255, 100);
	btn.Text = "[ ] " .. name;
	btn.Font = Enum.Font.Code;
	btn.TextSize = 14;
	btn.BorderSizePixel = 0;
	btn.ZIndex = 7;
	local bStroke = Instance.new("UIStroke", btn);
	bStroke.Color = Color3.fromRGB(0, 255, 100);
	bStroke.Thickness = 1;
	bStroke.Transparency = 0.6;
	local enabled = false;
	btn.MouseButton1Click:Connect(function()
		enabled = not enabled;
		if enabled then
			btn.TextColor3 = Color3.fromRGB(255, 255, 255);
			btn.BackgroundColor3 = Color3.fromRGB(0, 100, 40);
			btn.Text = "[X] " .. name;
			playSound(SOUNDS.Toggle, 0.5, 1.2);
		else
			btn.TextColor3 = Color3.fromRGB(0, 255, 100);
			btn.BackgroundColor3 = Color3.fromRGB(15, 15, 15);
			btn.Text = "[ ] " .. name;
			playSound(SOUNDS.Static, 0.5, 0.8);
		end
		callback(enabled);
		if name:find("SIZE") then
			btn.Text = "[ ] FONT SIZE: " .. espTextSize;
		elseif name:find("BIND") then
			if isBindingESP then
				btn.Text = "[ ] PRESS ANY KEY";
			else
				btn.Text = "[ ] BIND: " .. ESP_KEY.Name;
			end
		end
	end);
	return btn;
end
