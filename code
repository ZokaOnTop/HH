	local cleanText = speedInput.Text:gsub("%D+", "");
	speedInput.Text = cleanText;
	customSpeedValue = tonumber(cleanText) or 16;
end);
createButton("ACTIVATE SPEED", playerGrid, function(state)
	walkSpeedActive = state;
	task.spawn(function()
		while walkSpeedActive do
			local hum = player.Character and player.Character:FindFirstChild("Humanoid");
			if hum then
				hum.WalkSpeed = customSpeedValue;
			end
			task.wait(0.1);
		end
		local hum = player.Character and player.Character:FindFirstChild("Humanoid");
		if hum then
			hum.WalkSpeed = 16;
		end
	end);
end);
local autoFarmGrid = createCategory("AUTOFARM");
local autoLootRunning = false;
createButton("AUTO LOOT TRASH", autoFarmGrid, function(state)
	autoLootRunning = state;
	local targetPos = Vector3.new(-142.07650756835938, 10.118797302246094, -34.004337310791016);
	local lootPath = workspace:FindFirstChild("Loot");
	local dumpster = lootPath and lootPath:FindFirstChild("Dumpster3");
	local cont = dumpster and dumpster:FindFirstChild("Cont");
	local prompt = cont and cont:FindFirstChild("ProximityPrompt");
	if state then
		if (player.Character and player.Character:FindFirstChild("HumanoidRootPart")) then
			player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPos);
		end
		task.spawn(function()
			while autoLootRunning do
				if prompt then
					prompt.HoldDuration = 0;
					for i = 1, 3 do
						if not autoLootRunning then
							break;
						end
						fireproximityprompt(prompt);
						task.wait(1);
					end
				end
				task.wait(118);
			end
		end);
	elseif prompt then
		prompt.HoldDuration = 1;
	end
end);
local autoCleaning = false;
createButton("AUTO CLEAN DELI", autoFarmGrid, function(state)
	autoCleaning = state;
	if state then
		task.spawn(function()
			if (player.Character and player.Character:FindFirstChild("HumanoidRootPart")) then
				player.Character.HumanoidRootPart.CFrame = CFrame.new(-559.367, 8.102, -90.45);
				task.wait(0.5);
				local tcs = game:GetService("TextChatService");
				if (tcs.ChatVersion == Enum.ChatVersion.TextChatService) then
					tcs.TextChannels.RBXGeneral:SendAsync("/applyjob");
				else
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/applyjob", "All");
				end
				task.wait(1);
				player.Character.HumanoidRootPart.CFrame = CFrame.new(-556.981, 9.496, -85.429);
				task.wait(0.5);
			end
			while autoCleaning do
				local cleaningJob = workspace:FindFirstChild("JobSystem") and workspace.JobSystem:FindFirstChild("CleaningJob2");
				if cleaningJob then
					for _, spot in pairs(cleaningJob:GetChildren()) do
						if not autoCleaning then
							break;
						end
						if (spot.Name == "CleaningSpot") then
							local prompt = spot:FindFirstChildWhichIsA("ProximityPrompt");
							if prompt then
								prompt.HoldDuration = 0;
								prompt.Enabled = true;
								prompt.RequiresLineOfSight = false;
								task.spawn(function()
									pcall(function()
										fireproximityprompt(prompt);
									end);
								end);
							end
						end
					end
				end
				task.wait(0.3);
			end
		end);
	else
		local cleaningJob = workspace:FindFirstChild("JobSystem") and workspace.JobSystem:FindFirstChild("CleaningJob2");
		if cleaningJob then
			for _, spot in pairs(cleaningJob:GetChildren()) do
				local prompt = spot:FindFirstChildWhichIsA("ProximityPrompt");
				if prompt then
					prompt.HoldDuration = 5.5;
				end
			end
		end
	end
end);
local autoChicken = false;
createButton("AUTO CLEAN CHICKEN", autoFarmGrid, function(state)
	autoChicken = state;
	if state then
		task.spawn(function()
			if (player.Character and player.Character:FindFirstChild("HumanoidRootPart")) then
				player.Character.HumanoidRootPart.CFrame = CFrame.new(-176.825, 4.163, 143.761);
				task.wait(0.5);
				local tcs = game:GetService("TextChatService");
				if (tcs.ChatVersion == Enum.ChatVersion.TextChatService) then
					tcs.TextChannels.RBXGeneral:SendAsync("/applyjob");
				else
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/applyjob", "All");
				end
				task.wait(1);
				player.Character.HumanoidRootPart.CFrame = CFrame.new(-192.014, 4.163, 132.605);
				task.wait(0.5);
			end
			while autoChicken do
				local cleaningJob = workspace:FindFirstChild("JobSystem") and workspace.JobSystem:FindFirstChild("CleaningJob4");
				if cleaningJob then
					for _, spot in pairs(cleaningJob:GetChildren()) do
						if not autoChicken then
							break;
						end
						if (spot.Name == "CleaningSpot") then
							local prompt = spot:FindFirstChildWhichIsA("ProximityPrompt");
							if prompt then
								prompt.HoldDuration = 0;
								if not prompt.Enabled then
									prompt.Enabled = true;
								end
								task.spawn(function()
									fireproximityprompt(prompt);
								end);
							end
						end
					end
				end
				task.wait(0.01);
			end
		end);
	else
		local cleaningJob = workspace:FindFirstChild("JobSystem") and workspace.JobSystem:FindFirstChild("CleaningJob4");
		if cleaningJob then
			for _, spot in pairs(cleaningJob:GetChildren()) do
				if (spot.Name == "CleaningSpot") then
					local prompt = spot:FindFirstChildWhichIsA("ProximityPrompt");
					if prompt then
						prompt.HoldDuration = 5.5;
					end
				end
			end
		end
	end
end);
local autoChicken2 = false;
createButton("AUTO CLEAN MODO'S", autoFarmGrid, function(state)
	autoChicken2 = state;
	if state then
		task.spawn(function()
			if (player.Character and player.Character:FindFirstChild("HumanoidRootPart")) then
				player.Character.HumanoidRootPart.CFrame = CFrame.new(-171.276, 17.122, -79.654);
				task.wait(0.5);
				local tcs = game:GetService("TextChatService");
				if (tcs.ChatVersion == Enum.ChatVersion.TextChatService) then
					tcs.TextChannels.RBXGeneral:SendAsync("/applyjob");
				else
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/applyjob", "All");
				end
				task.wait(1);
				player.Character.HumanoidRootPart.CFrame = CFrame.new(-186.963, 17.108, -67.045);
				task.wait(0.5);
			end
			while autoChicken2 do
				local cleaningJob = workspace:FindFirstChild("JobSystem") and workspace.JobSystem:FindFirstChild("CleaningJob3");
				if cleaningJob then
					for _, spot in pairs(cleaningJob:GetChildren()) do
						if not autoChicken2 then
							break;
						end
						if (spot.Name == "CleaningSpot") then
							local prompt = spot:FindFirstChildWhichIsA("ProximityPrompt");
							if prompt then
								prompt.HoldDuration = 0;
								if not prompt.Enabled then
									prompt.Enabled = true;
								end
								task.spawn(function()
									fireproximityprompt(prompt);
								end);
							end
						end
					end
				end
				task.wait(0.01);
			end
		end);
	else
		local cleaningJob = workspace:FindFirstChild("JobSystem") and workspace.JobSystem:FindFirstChild("CleaningJob3");
		if cleaningJob then
			for _, spot in pairs(cleaningJob:GetChildren()) do
				if (spot.Name == "CleaningSpot") then
					local prompt = spot:FindFirstChildWhichIsA("ProximityPrompt");
					if prompt then
						prompt.HoldDuration = 5.5;
					end
				end
			end
		end
	end
end);
local flyJumpPower = 50;
local flyJumpEnabled = false;
local flyJumpConnection = nil;
createButton("INF JUMP", playerGrid, function(state)
	flyJumpEnabled = state;
	if flyJumpEnabled then
		flyJumpConnection = game:GetService("UserInputService").InputBegan:Connect(function(input, gpe)
			if (not gpe and (input.KeyCode == Enum.KeyCode.Space)) then
				if (flyJumpEnabled and player.Character and player.Character:FindFirstChild("HumanoidRootPart")) then
					local hrp = player.Character.HumanoidRootPart;
					hrp.Velocity = Vector3.new(hrp.Velocity.X, flyJumpPower, hrp.Velocity.Z);
					playSound(SOUNDS.Toggle, 0.2, 2);
				end
			end
		end);
	else
		if flyJumpConnection then
			flyJumpConnection:Disconnect();
			flyJumpConnection = nil;
		end
		playSound(SOUNDS.Static, 0.5, 0.8);
	end
end);
createButton("NOCLIP", playerGrid, function(state)
	noclipEnabled = state;
	if noclipEnabled then
		noclipConnection = game:GetService("RunService").Stepped:Connect(function()
			if (noclipEnabled and player.Character) then
				for _, part in pairs(player.Character:GetDescendants()) do
					if (part:IsA("BasePart") and part.CanCollide) then
						part.CanCollide = false;
					end
				end
			end
		end);
		playSound(SOUNDS.Toggle, 0.5, 1.5);
	else
		if noclipConnection then
			noclipConnection:Disconnect();
			noclipConnection = nil;
		end
		if player.Character then
			for _, part in pairs(player.Character:GetDescendants()) do
				if part:IsA("BasePart") then
					part.CanCollide = true;
				end
			end
		end
		playSound(SOUNDS.Static, 0.5, 0.8);
	end
end);
local visualGrid = createCategory("VISUALS");
local espMainBtn = createButton("SYSTEM ESP", visualGrid, function(state)
	updateESPStatus(state);
end);
createButton("SHOW DISTANCE", visualGrid, function(state)
	showDistance = state;
end);
createButton("SHOW HEALTH", visualGrid, function(state)
	showHealth = state;
end);
local sizeBtn = createButton("FONT SIZE: " .. espTextSize, visualGrid, function()
	if (espTextSize >= 20) then
		espTextSize = 8;
	else
		espTextSize = espTextSize + 2;
	end
end);
local bindBtn = createButton("BIND: " .. ESP_KEY.Name, visualGrid, function()
	isBindingESP = true;
end);
local utilityGrid = createCategory("UTILITY");
createButton("FULLBRIGHT", utilityGrid, function(state)
	fullBrightEnabled = state;
	if fullBrightEnabled then
		game:GetService("Lighting").Ambient = Color3.fromRGB(255, 255, 255);
		game:GetService("Lighting").OutdoorAmbient = Color3.fromRGB(255, 255, 255);
		game:GetService("Lighting").Brightness = 2;
	else
		game:GetService("Lighting").Ambient = oldAmbient;
		game:GetService("Lighting").OutdoorAmbient = oldOutdoorAmbient;
		game:GetService("Lighting").Brightness = 1;
	end
end);
createButton("ANTI-AFK", utilityGrid, function(state)
	local connections = getconnections or get_signal_cons;
	if connections then
		for _, v in pairs(connections(player.Idled)) do
			if state then
				v:Disable();
			else
				v:Enable();
			end
		end
	end
	if state then
		player.Idled:Connect(function()
			game:GetService("VirtualUser"):CaptureController();
			game:GetService("VirtualUser"):ClickButton2(Vector2.new());
		end);
	end
end);
createButton("REJOIN SERVER", utilityGrid, function()
	local ts = game:GetService("TeleportService");
	ts:TeleportToPlaceInstance(game.PlaceId, game.JobId, player);
end);
createButton("FPS BOOSTER", utilityGrid, function()
	for _, v in pairs(game:GetDescendants()) do
		if (v:IsA("BasePart") and not v:IsA("MeshPart")) then
			v.Material = Enum.Material.SmoothPlastic;
		elseif (v:IsA("Decal") or v:IsA("Texture")) then
			v:Destroy();
		end
	end
end);
local aimbotBtn = createButton("AIMBOT HUB", utilityGrid, function()
	playSound(SOUNDS.AimbotHub, 0.2, 2);
	loadstring(game:HttpGet("https://raw.githubusercontent.com/ZokaOnTop/005/refs/heads/main/005"))();
end);
aimbotBtn.TextColor3 = Color3.fromRGB(255, 0, 0);
local stroke = aimbotBtn:FindFirstChildWhichIsA("UIStroke");
if stroke then
	stroke.Color = Color3.fromRGB(255, 0, 0);
end
local dragging, dragInput, dragStart, startPos;
mainFrame.InputBegan:Connect(function(input)
	if ((input.UserInputType == Enum.UserInputType.MouseButton1) or (input.UserInputType == Enum.UserInputType.Touch)) then
		dragging = true;
		dragStart = input.Position;
		startPos = mainFrame.Position;
		input.Changed:Connect(function()
			if (input.UserInputState == Enum.UserInputState.End) then
				dragging = false;
			end
		end);
	end
end);
mainFrame.InputChanged:Connect(function(input)
	if ((input.UserInputType == Enum.UserInputType.MouseMovement) or (input.UserInputType == Enum.UserInputType.Touch)) then
		dragInput = input;
	end
end);
UserInputService.InputChanged:Connect(function(input)
	if ((input == dragInput) and dragging) then
		local delta = input.Position - dragStart;
		mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y);
	end
end);
function toggleUI()
	if not isOpen then
		isOpen = true;
		playSound(SOUNDS.Toggle, 0.7, 1);
		local targetPos = mainFrame.Position;
		local absolutePos = mainFrame.AbsolutePosition;
		local absoluteSize = mainFrame.AbsoluteSize;
		fullScreenGlitch();
		for i = 1, 5 do
			task.spawn(function()
				local pixel = Instance.new("Frame", screenGui);
				pixel.BorderSizePixel = 0;
				pixel.BackgroundColor3 = ((math.random() > 0.5) and Color3.fromRGB(0, 255, 100)) or Color3.fromRGB(255, 255, 255);
				pixel.Size = UDim2.new(0, 0, 0, 0);
				pixel.ZIndex = 10001;
				local startX = absolutePos.X + (absoluteSize.X / 2) + math.random(-300, 300);
				local startY = absolutePos.Y + (absoluteSize.Y / 2) + math.random(-300, 300);
				pixel.Position = UDim2.new(0, startX, 0, startY);
				pixel.BackgroundTransparency = 1;
				local destX = absolutePos.X + math.random(0, absoluteSize.X);
				local destY = absolutePos.Y + math.random(0, absoluteSize.Y);
				local tween = TweenService:Create(pixel, TweenInfo.new(0.5, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Position=UDim2.new(0, destX, 0, destY),Size=UDim2.new(0, math.random(5, 25), 0, math.random(5, 25)),BackgroundTransparency=0.2,Rotation=math.random(-180, 180)});
				tween:Play();
				tween.Completed:Connect(function()
					task.wait(0.05);
					pixel:Destroy();
				end);
			end);
			if ((i % 6) == 0) then
				task.wait();
			end
		end
		task.wait(0.2);
		mainFrame.Visible = true;
		fullScreenGlitch();
		for i = 1, 8 do
			mainFrame.Position = UDim2.new(targetPos.X.Scale, targetPos.X.Offset + math.random(-5, 5), targetPos.Y.Scale, targetPos.Y.Offset + math.random(-5, 5));
			task.wait(0.01);
		end
		mainFrame.Position = targetPos;
	else
		isOpen = false;
		playSound(SOUNDS.Static, 0.8, 0.8);
		local absolutePos = mainFrame.AbsolutePosition;
		local absoluteSize = mainFrame.AbsoluteSize;
		mainFrame.Visible = false;
		fullScreenGlitch();
		for i = 1, 25 do
			task.spawn(function()
				local pixel = Instance.new("Frame", screenGui);
				pixel.BorderSizePixel = 0;
				pixel.BackgroundColor3 = ((math.random() > 0.5) and Color3.fromRGB(0, 255, 100)) or Color3.fromRGB(20, 20, 20);
				pixel.Size = UDim2.new(0, math.random(10, 40), 0, math.random(10, 40));
				pixel.Position = UDim2.new(0, absolutePos.X + math.random(0, absoluteSize.X), 0, absolutePos.Y + math.random(0, absoluteSize.Y));
				pixel.ZIndex = 10001;
				local drift = math.random(-150, 150);
				local fall = math.random(200, 500);
				local tween = TweenService:Create(pixel, TweenInfo.new(0.6, Enum.EasingStyle.Quart, Enum.EasingDirection.In), {Position=(pixel.Position + UDim2.new(0, drift, 0, fall)),Rotation=math.random(-180, 180),BackgroundTransparency=1,Size=UDim2.new(0, 0, 0, 0)});
				tween:Play();
				tween.Completed:Connect(function()
					pixel:Destroy();
				end);
			end);
			if ((i % 5) == 0) then
				task.wait();
			end
		end
	end
end
function startIntro()
	task.wait(0.8);
	toggleUI();
end
task.spawn(startIntro);
local isToggling = false;
UserInputService.InputBegan:Connect(function(input, gpe)
	if (isBindingESP and (input.UserInputType == Enum.UserInputType.Keyboard)) then
		ESP_KEY = input.KeyCode;
		isBindingESP = false;
		playSound(SOUNDS.Toggle, 0.5, 1.5);
		local visualGrid = container:FindFirstChild("VISUALS_Grid");
		if visualGrid then
			for _, child in pairs(visualGrid:GetChildren()) do
				if (child:IsA("TextButton") and child.Name:find("BIND")) then
					child.Text = "[ ] BIND: " .. ESP_KEY.Name;
				end
			end
		end
		return;
	end
	if not gpe then
		if (input.KeyCode == ALT_TOGGLE) then
			if isToggling then
				return;
			end
			isToggling = true;
			toggleUI();
			task.wait(0.5);
			isToggling = false;
		elseif (input.KeyCode == ESP_KEY) then
			if isToggling then
				return;
			end
			isToggling = true;
			updateESPStatus(not espEnabled);
			task.wait(0.5);
			isToggling = false;
		end
	end
end);
