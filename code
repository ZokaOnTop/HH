local SOUNDS = {IntroAmbience="rbxassetid://90305812327722",Static="rbxassetid://140043289814504",Toggle="rbxassetid://80099403859001",AimbotHub="rbxassetid://110205432201626",IntroExtra="rbxassetid://18379039436",Lagging="rbxassetid://91797564915224"};
